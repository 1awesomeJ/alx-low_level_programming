#include <stdio.h>

int if_prime(int n)
	{
		int i = n/2;
		int k;
		int p;
		for (k = 2; k <=i; k++)
		if (n % k != 0)
		{
		p = n;
		}
		else
		{
		p = 0;
		}
		return (p);
	}
int main(void)
{
	int a, b, c, d, e, f, g, h, i, j, k, ;
	a = 612852475143/2;
		b = if_prime (a);
	if b > 0	
		printf("%d\n", b);
	else 
	c =b/2;
		d= if_prime(c);
		if d > 0 
		printf("%d", d)
}
